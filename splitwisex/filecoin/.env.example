# Company Wallet Configuration
# ⚠️ NEVER commit the actual private key to git!
COMPANY_PRIVATE_KEY=0x_your_company_wallet_private_key_here

# Filecoin Network Configuration
FILECOIN_NETWORK=calibration
FILECOIN_RPC_HTTP=https://api.calibration.node.glif.io/rpc/v1
FILECOIN_RPC_WS=wss://api.calibration.node.glif.io/rpc/v1

# Storage Service Configuration (adjust based on your needs)
USDFC_PER_EPOCH=50              # USDFC per epoch (default: 50, you can adjust)
USDFC_LOCKUP=200                # USDFC lockup amount (default: 200, you can adjust)
LOCKUP_DAYS=90                  # Lockup duration in days (default: 90, you can adjust)

# Optional: GLIF token for higher rate limits
GLIF_TOKEN=your_glif_token_here